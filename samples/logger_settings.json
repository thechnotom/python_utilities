{
    "do_logging": true,
    "do_type": true,
    "do_location": true,
    "do_short_location": true,
    "do_timestamp": true,
    "do_type_missing_indicator": true,
    "strict_types": false,
    "console": {
        "enable": true
    },
    "file": {
        "enable": true,
        "clear": true,
        "output_filename": "logged_output.txt",
        "max_file_size": 1000000,
        "max_backups": 1
    },
    "types": {
        "info": true,
        "warning": true,
        "error": true
    }
}